    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Product Page</title>
    <!-- Add your CSS links here -->
    <link rel="stylesheet" href="/style/style.css">
</head>
<body>
    <div class="products">
        <!-- Product 1 -->
        <div class="product">
